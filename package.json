    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "litepicker": "^2.0.12",
    "luxon": "^3.4.4",
    "node-thermal-printer": "^4.4.2",
    "react": "^18.3.1",
    "react-activity": "^2.1.3",
