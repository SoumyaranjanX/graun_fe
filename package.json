    "@mui/x-date-pickers-pro": "^7.4.0",
    "@stripe/react-stripe-js": "^2.7.1",
    "@stripe/stripe-js": "^3.4.0",
    "@types/firebase": "^3.2.1",
    "antd": "^5.19.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
