    "@types/react-dom": "^18.3.0",
    "@types/react-modal": "^3.16.3",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
